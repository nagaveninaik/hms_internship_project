
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>view department</title>
	<!-- <link rel=“icon” type=“image/x-icon” href=“/image/call.ico”> -->
	<link rel="icon" href="/heart.ico" type="image/icon type">

	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="/style.css" />
    <style>

body{
    background-image: url("/image/home.png");
    background-repeat: no-repeat;
    background-size: cover;
}


        
        
    </style>
</head>
<body>
	<%- include("common/header.ejs") %>
    <%- include("common/nav.ejs") %>
    <center>
    <table border="2">
        <tr>
            <th>dept_id</th>
            <th>dept_name</th>
            <th>start_date</th>

            <th>Delete</th>
            
        </tr>
        <% result.forEach((data=>{ %>
            <tr>
              <td><%= data._id %></td>
              <td><%= data.dept_name %></td>
              <td><%= data.start_date %></td>
        <td><a href="/viewdepartment/<%= data._id%>" class="delete"><img src="/image/delete.ico" alt="delete"></a></td>     
              
            
            </tr>    
        <% })) %> 
        
     
       </table></center>
       <%- include("common/footer.ejs") %>
</body>
</html>
