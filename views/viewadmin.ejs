
 
    
 
 <!DOCTYPE html>
 <html lang="en">
 <head>
     <meta charset="UTF-8">
     <meta http-equiv="X-UA-Compatible" content="IE=edge">
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>Document</title>
 </head>
 <body>
     
  

<table border="2">
    <tr>
        <th>name</th>
        <th>ph_no</th>
        <th>date_of_birth</th>
        <th>gender</th>
        <th>address</th>
        <th>email</th>
        <th>joining_date</th>
        <th>salary</th>
    </tr>
    <% result.forEach((data=>{ %>
        <tr>
          <td><%= data.name %></td>
          <td><%= data.ph_no %></td>
          <td><%= data.date_of_birth %></td>
          <td><%= data.gender%></td>
          <td><%= data.address%></td>
          <td><%= data.email%></td>
          <td><%= data.joining_date%></td>
          <td><%= data.salary%></td>
        
        </tr>    
    <% })) %> 
    
 
   </table>
 </body>
 </html>
