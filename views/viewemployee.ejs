<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>view employee</title>
	<!-- <link rel=“icon” type=“image/x-icon” href=“/image/call.ico”> -->
	<link rel="icon" href="/heart.ico" type="image/icon type">

	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="/style.css" />
    
</head>
<body>
    <%- include("common/header.ejs") %>
    <%- include("common/nav.ejs") %>
    
    <center>
<table border="2">
    <tr>
        <th>employee_id</th>
        <th>name</th>
        <th>ph_no</th>
        <th>date_of_birth</th>
        <th>gender</th>
        <th>address</th>
        <th>email</th>
        <th>joining_date</th>
        <th>salary</th>
        <th>role</th>
        <th>print</th>
        <th>edit</th>
        <th>delete</th>
        
    </tr>
    <% result.forEach((data)=>{ %>
        <tr>
            <td><%= data._id %></td>
          <td><%= data.name %></td>
          <td><%= data.ph_no %></td>
          <td><%= data.date_of_birth %></td>
          <td><%= data.gender%></td>
          <td><%= data.address%></td>
          <td><%= data.email%></td>
          <td><%= data.joining_date%></td>
          <td><%= data.salary%></td>
          <td><%= data.role%></td>
          <td><a href="/copy_employee/<%= data._id%>">print</a></td>
         <td><a href="/editemployee/<%= data._id%>"><img src="/image/edit.ico" alt="edit" width="40%" height="40%"></a></td>
          <td><a href="/viewemployee/<%= data._id%>" class="delete"><img src="/image/delete.ico" alt="delete"></a></td>
          
        
        </tr>    
    <% }) %> 
    
 
   </table>
</center>
<%- include("common/footer.ejs") %>
	
	
</body>
</html>
