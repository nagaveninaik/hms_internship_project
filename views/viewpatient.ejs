

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>view patient</title>
	<link rel="icon" href="/heart.ico" type="image/icon type">

	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="/style.css" />
    <style>

body{
    background-image: url("/image/home.png");
    background-repeat: no-repeat;
    background-size: cover;
}


        
        
    </style>
</head>
<body>
	<%- include("common/header.ejs") %>
	<%- include("common/navbar.ejs") %>
    
<table border="2">
    <tr>
        <th>patient_id</th>
        <th>name</th>
        <th>ph_no</th>
        <th>email</th>
        <th>date_of_birth</th>
        <th>gender</th>
        <th>address</th>
        
        <th>blood_group</th>
		<th>Print</th>
        <th>Edit</th>
        <th>Delete</th>
        
    </tr>
    <% result.forEach((data)=>{ %>
        <tr>
          <td><%= data._id%></td>
          <td><%= data.name %></td>
          <td><%= data.ph_no %></td>
          <td><%= data.email%></td>
          <td><%= data.date_of_birth %></td>
          <td><%= data.gender%></td>
          <td><%= data.address%></td>
          
          <td><%= data.blood_group%></td>
		  <td><a href="/copy/<%= data._id%>">print</a></td>
         <td><a href="/editpatient/<%= data._id%>"><img src="/image/edit.ico" alt="edit" width="40%" height="40%"></a></td>
         <td><a href="/viewpatient/<%= data._id%>" class="delete"><img src="/image/delete.ico" alt="delete"></a></td>
         
        </tr> 
    <% }) %> 
    
 
   </table>
   <%- include("common/footer.ejs") %>
	
	
</body>
</html>

